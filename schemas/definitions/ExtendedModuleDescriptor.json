  "required": [
    "module_id",
    "name",
    "version",
    "interfaces",
    "resource_cost",
    "status"
  ],
  "properties": {
    "module_id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "version": {
      "type": "string"
    },
    "interfaces": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "resource_cost": {
      "type": "object",
      "properties": {
        "cpu": {
          "type": "number"
        },
        "gpu": {
          "type": "string"
        },
        "ram_mb": {
          "type": "integer"
        },
        "disk_mb": {
          "type": "integer"
        },
        "network_mbps": {
          "type": "integer"
        }
      }
    },
    "status": {
      "type": "string"
    },
    "last_failure": {
      "type": "string",
      "format": "date-time"
    },
