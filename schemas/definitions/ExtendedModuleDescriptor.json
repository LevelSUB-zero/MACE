      "type": "object"
    },
    "owner": {
      "type": "string"
    },
    "cost_per_hour": {
      "type": "number"
    },
    "provenance": {
      "type": "object",
      "properties": {
        "source_repo": {
          "type": "string"
        },
        "commit_hash": {
          "type": "string"
        },
        "build_id": {
          "type": "string"
        }
      }
    },
